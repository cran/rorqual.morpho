Type: Package
Package: rorqual.morpho
Title: Morphological Allometry of Rorquals
Version: 0.1.1
Authors@R: c(
    person(given = "Max",
           family = "Czapanskiy",
           role = c("aut", "cre"),
           email = "maxczapanskiy@gmail.com",
           comment = c("ORCID-ID" = "0000-0002-6302-905X")),
    person(given = "Shirel",
           family = "Kahane-Rapport",
           role = c("aut"),
